#set($stream_name="AdWordsJump")
#macro(logtable $docname)
	#set($bentrydoc = $xwiki.getDocument($docname))
    #set($log_infos=$xwiki.search("from ActivityEventImpl as act where act.stream='${stream_name}' and act.type='Log' and act.page='${docname}' order by param1"))
	#subtitlebar(${bentrydoc.name} "" "" "purple")
	&lt;table border="1" cellspacing="0" bgcolor="#000000" width="100%"&gt;
		&lt;tr bgcolor="#CFCFCF" height="30"&gt;
			&lt;td width="150"&gt;&lt;b&gt;$msg.get("AdWordsJump.LandingPageLogTemplate.label.searchTime")&lt;/b&gt;&lt;/td&gt;
			&lt;td&gt;&lt;b&gt;$msg.get("AdWordsJump.LandingPageLogTemplate.label.searchText")&lt;/b&gt;&lt;/td&gt;
			&lt;td width="80"&gt;&lt;b&gt;$msg.get("AdWordsJump.LandingPageLogTemplate.label.inform")&lt;/b&gt;&lt;/td&gt;
			&lt;td width="180"&gt;&lt;b&gt;$msg.get("AdWordsJump.LandingPageLogTemplate.label.email")&lt;/b&gt;&lt;/td&gt;
		&lt;/tr&gt;
	#foreach ($log_info in $log_infos)
		&lt;tr bgcolor="#FFFFFF" height="25"&gt;&lt;td&gt;$log_info.param1&lt;/td&gt;&lt;td&gt;$log_info.param2&lt;/td&gt;&lt;td&gt;$log_info.param3&lt;/td&gt;&lt;td&gt;$log_info.param4&lt;/td&gt;&lt;/tr&gt;
	 #end
	&lt;/table&gt;
#end
#if($request.docname &amp;&amp; $request.docname == "all_log_docs")
    #set ($sql = " where doc.parent='AdWordsJump.LandingPageLog'")
    #set ($items = $xwiki.searchDocuments($sql))
    #foreach ($item in $items)
        #logtable($item)
    #end
#elseif($request.docname)
	#logtable($request.docname)
