&lt;script type="text/javascript"&gt;
var $hidden = false;
function toggle_desc(){
  var $action;
  var $titl;
  if ($hidden){
    $hidden = false;
    $action = 'show';
    $titl = "Hide Descriptions";
  } else {
    $hidden = true;
    $action = 'hide';
    $titl = "Show Descriptions";
  }
  $$('.mycurriki-collections-row-description').invoke($action);
  $('hider-link').title=$titl;
  $('hider-link').innerHTML=$titl+"  &amp;raquo;";
  return false;
}
&lt;/script&gt;

#mycurrikifulltitlebar(Curriculum Collections Hide Descriptions "" "return toggle_desc();" "red" "hider")
 &lt;div class="centertext"&gt;&lt;/div&gt;
 #mycurrikipaginator(Collections -1 20 0)
