	var callback = null;
    function onCheckClick(eventType, args, tree) {
        var out = [];

        // Force checking again the main element
        tree4.getRoot().children[0].check()
        tree4.getRoot().children[0].expand()

        for (var i in tree4._nodes) {
            var n = tree4._nodes[i];
            if (n &amp;&amp; "undefined" != typeof n.checkState) {
                if (n.checkState&gt;0) {
                  out.push(n.data);
                }
            }
        }
        document.getElementById("XWiki.XWikiUsers_0_topics").value=out.join("|");
    }

    // Init the tree
    treeInit4();
&lt;/script&gt;
