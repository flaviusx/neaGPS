1 largepanelcolor()
&lt;pre&gt;
largepanelcolor("panel title" "color")
 yout html text here
panelfooter()
&lt;/pre&gt;

#largepanelcolor("purple panel title" "purple")
your html text &lt;b&gt;here&lt;/b&gt;
#panelfooter()
#largepanelcolor("blue panel title" "blue")
your html text &lt;b&gt;here&lt;/b&gt;
#panelfooter()
#largepanelcolor("green panel title" "green")
your html text &lt;b&gt;here&lt;/b&gt;
#panelfooter()
#largepanelcolor("orange panel title" "orange")
your html text &lt;b&gt;here&lt;/b&gt;
#panelfooter()
#largepanelcolor("red panel title" "red")
your html text &lt;b&gt;here&lt;/b&gt;
#panelfooter()
#largepanelcolor("black panel title" "black")
your html text &lt;b&gt;here&lt;/b&gt;
#panelfooter()

