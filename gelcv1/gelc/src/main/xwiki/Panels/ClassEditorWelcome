<content>#largepanelheader($msg.get("classEditorIntro"))
&lt;p&gt;You are editing &lt;strong&gt;${doc.fullName}&lt;/strong&gt;. Choose a property to edit or add a property to the class.&lt;/p&gt;
&lt;div&gt;
#set($rsvc= $xwiki.xWiki.getRightService())
&lt;form id="changeclass" method="post" action="$doc.getURL("redirect")"&gt;
&lt;div class="centered"&gt;
&lt;select id="classname" name="xredirect" size="1"&gt;
&lt;option value="---"&gt;--Edit another class--&lt;/option&gt;
#foreach ($classname in $xwiki.classList)
#if($!xwiki.hasAccessLevel("edit", $classname))
#set($classdoc = $xwiki.getDocument($classname))
&lt;option value="$classdoc.getURL("edit", "editor=class")"&gt;$classname&lt;/option&gt;
#end
#end
&lt;/select&gt;
&lt;script type="text/javascript"&gt;
function gotoclass(dest) {
 if (dest!="---") {
   location = dest;
   }
   return false;
}
&lt;/script&gt;
&lt;input class="button" type="submit" value="$msg.get("changeclass")" onclick="return gotoclass(this.form.classname.value)" /&gt;&lt;/div&gt;
##&lt;div&gt;(unsaved changes will be lost)&lt;/div&gt;
&lt;/form&gt;
&lt;/div&gt;
